GET {{blockfrost_base_url}}/addresses/{{test_address}}/total
project_id: {{blockfrost_project_id}}

> {%
    client.global.set("blockfrostAddressTotalResponse", response.body);

    client.test("Get address total", function() {
        client.assert(response.status === 200, "Response status is not 200");
    });
%}

###
# @timeout 6000
GET {{base_url}}/api/v1/blockfrost/addresses/{{test_address}}/total

> {%
    const blockfrostResponse = client.global.get("blockfrostAddressTotalResponse");
    const yaciResponse = response.body;

    client.test("Compare address total between Blockfrost and Yaci", function() {
        client.assert(response.status === 200, "Response status is not 200");

        // Compare address
        client.assert(blockfrostResponse.address === yaciResponse.address, `address is different (Blockfrost: ${blockfrostResponse.address}, Yaci: ${yaciResponse.address})`);

        // Compare received_sum
        client.assert(Array.isArray(blockfrostResponse.received_sum), "Blockfrost received_sum should be an array");
        client.assert(Array.isArray(yaciResponse.received_sum), "Yaci received_sum should be an array");
        client.assert(blockfrostResponse.received_sum.length === yaciResponse.received_sum.length, `received_sum length is different (Blockfrost: ${blockfrostResponse.received_sum.length}, Yaci: ${yaciResponse.received_sum.length})`);

        if (blockfrostResponse.received_sum.length > 0 && yaciResponse.received_sum.length > 0) {
            client.assert(blockfrostResponse.received_sum[0].unit === yaciResponse.received_sum[0].unit, `received_sum[0].unit is different (Blockfrost: ${blockfrostResponse.received_sum[0].unit}, Yaci: ${yaciResponse.received_sum[0].unit})`);
            client.assert(blockfrostResponse.received_sum[0].quantity === yaciResponse.received_sum[0].quantity, `received_sum[0].quantity is different (Blockfrost: ${blockfrostResponse.received_sum[0].quantity}, Yaci: ${yaciResponse.received_sum[0].quantity})`);
        }

        // Compare sent_sum
        client.assert(Array.isArray(blockfrostResponse.sent_sum), "Blockfrost sent_sum should be an array");
        client.assert(Array.isArray(yaciResponse.sent_sum), "Yaci sent_sum should be an array");
        client.assert(blockfrostResponse.sent_sum.length === yaciResponse.sent_sum.length, `sent_sum length is different (Blockfrost: ${blockfrostResponse.sent_sum.length}, Yaci: ${yaciResponse.sent_sum.length})`);

        if (blockfrostResponse.sent_sum.length > 0 && yaciResponse.sent_sum.length > 0) {
            client.assert(blockfrostResponse.sent_sum[0].unit === yaciResponse.sent_sum[0].unit, `sent_sum[0].unit is different (Blockfrost: ${blockfrostResponse.sent_sum[0].unit}, Yaci: ${yaciResponse.sent_sum[0].unit})`);
            client.assert(blockfrostResponse.sent_sum[0].quantity === yaciResponse.sent_sum[0].quantity, `sent_sum[0].quantity is different (Blockfrost: ${blockfrostResponse.sent_sum[0].quantity}, Yaci: ${yaciResponse.sent_sum[0].quantity})`);
        }

        // Compare tx_count
        client.assert(blockfrostResponse.tx_count === yaciResponse.tx_count, `tx_count is different (Blockfrost: ${blockfrostResponse.tx_count}, Yaci: ${yaciResponse.tx_count})`);
    });
%}


